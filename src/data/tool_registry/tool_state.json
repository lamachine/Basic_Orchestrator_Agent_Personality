{
  "last_updated": "2025-05-07T00:40:52.764973",
  "configs": {
    "personal_assistant": {
      "name": "personal_assistant",
      "description": "Handles emails, messages, to-do lists, and calendar integration.",
      "version": "0.1.0",
      "config": {
        "default_email": "user@example.com",
        "max_retries": 3,
        "timeout_seconds": 30,
        "default_timezone": "UTC"
      },
      "capabilities": [
        "email",
        "calendar",
        "tasks",
        "reminders"
      ],
      "api": {
        "calendar": {
          "provider": "google",
          "scopes": [
            "https://www.googleapis.com/auth/calendar",
            "https://www.googleapis.com/auth/calendar.events"
          ],
          "auth_type": "oauth2"
        },
        "email": {
          "provider": "google",
          "scopes": [
            "https://www.googleapis.com/auth/gmail.readonly",
            "https://www.googleapis.com/auth/gmail.send"
          ],
          "auth_type": "oauth2"
        }
      },
      "metadata": {
        "author": "Basic Orchestrator",
        "created": "2024-05-15",
        "usage_examples": [
          "{\"task\": \"check my task list\"}",
          "{\"task\": \"add new task: Finish report for Smith project\"}",
          "{\"task\": \"check my email inbox\"}",
          "{\"task\": \"send an email to John about the meeting tomorrow\"}",
          "{\"task\": \"add a reminder to buy groceries at 6pm\"}",
          "{\"task\": \"check my calendar for next week\"}",
          "{\"task\": \"create a new task to finish the report\"}"
        ]
      }
    }
  }
}